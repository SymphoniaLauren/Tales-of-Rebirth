    <Entry>
      <PointerOffset>6652</PointerOffset>
      <VoiceId>2a000015</VoiceId>
      <JapaneseText>どうした、&lt;Mao&gt;？</JapaneseText>
      <EnglishText>What's wrong, &lt;Mao&gt;?</EnglishText>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>5</Id>
