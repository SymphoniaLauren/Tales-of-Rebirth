      <EnglishText>The one who is a
prisoner of honor.</EnglishText>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>16</Id>
      <Status>Proofreading</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>s2-5</Section>
