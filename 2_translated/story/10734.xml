    <Entry>
      <PointerOffset>3724</PointerOffset>
      <VoiceId>2f000011</VoiceId>
      <JapaneseText>うわぁぁぁぁぁぁぁぁッ!!</JapaneseText>
      <EnglishText>Aaaaaagh!!</EnglishText>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>25</Id>
      <Status>Proofreading</Status>
    </Entry>
    <Entry>
      <PointerOffset>3746</PointerOffset>
      <VoiceId>2f000012</VoiceId>
      <JapaneseText>お父さんッ!!</JapaneseText>
      <EnglishText>Father!</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>26</Id>
      <Status>Proofreading</Status>
    </Entry>
