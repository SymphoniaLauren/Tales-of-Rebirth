      <Notes/>
      <SpeakerId>1,2</SpeakerId>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4014</PointerOffset>
      <JapaneseText>まだまだ世界は秘密にみちておるぞ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1,2</SpeakerId>
      <Id>53</Id>
