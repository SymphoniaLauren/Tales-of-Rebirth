      <EnglishText/>
      <Notes/>
      <SpeakerId>1,2</SpeakerId>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3285</PointerOffset>
      <JapaneseText>何か役に立てるかもしれんのぉ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1,2</SpeakerId>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3342</PointerOffset>
