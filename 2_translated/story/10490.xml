      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>55</Id>
      <Status>Proofreading</Status>
    </Entry>
    <Entry>
      <PointerOffset>4310</PointerOffset>
      <VoiceId>0d000036</VoiceId>
      <JapaneseText>あ……待て……！</JapaneseText>
      <EnglishText>Hey... wait!</EnglishText>
      <Notes/>
