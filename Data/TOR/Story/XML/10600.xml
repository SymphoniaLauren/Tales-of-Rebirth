    <Entry>
      <PointerOffset>291</PointerOffset>
      <JapaneseText>ラジルダ・フォグマの家</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
