    <Entry>
      <PointerOffset>7472</PointerOffset>
      <VoiceId>3c00009d</VoiceId>
      <JapaneseText>……ありがとう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>161</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>7501</PointerOffset>
      <VoiceId>3c00009e</VoiceId>
      <JapaneseText>……妙だな。
