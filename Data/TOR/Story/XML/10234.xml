      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>162</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>7537</PointerOffset>
      <VoiceId>3c00009f</VoiceId>
