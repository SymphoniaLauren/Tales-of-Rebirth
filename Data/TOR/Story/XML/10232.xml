      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>469</PointerOffset>
      <JapaneseText>民家</JapaneseText>
      <EnglishText/>
