      <Notes/>
      <Id>67</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2147</PointerOffset>
      <JapaneseText>&lt;voice:33000051&gt;何するの!!</JapaneseText>
      <EnglishText>&lt;voice:33000051&gt;What are you doing!?</EnglishText>
      <Notes/>
      <Id>68</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
