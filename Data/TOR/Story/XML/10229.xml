      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4641</PointerOffset>
      <VoiceId>22000055</VoiceId>
      <JapaneseText>いや、あいつも……
