      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4517</PointerOffset>
      <VoiceId>22000052</VoiceId>
