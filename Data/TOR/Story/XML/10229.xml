      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5311</PointerOffset>
      <VoiceId>3c00000c</VoiceId>
