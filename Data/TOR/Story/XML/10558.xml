    <Entry>
      <PointerOffset>379</PointerOffset>
      <VoiceId>3b0000c9</VoiceId>
      <JapaneseText>来たか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>498</PointerOffset>
      <VoiceId>3b0000ca</VoiceId>
      <JapaneseText>…………!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>536</PointerOffset>
      <VoiceId>3b0000cb</VoiceId>
      <JapaneseText>おまえを見てるとな、
ムカムカするんだよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>567</PointerOffset>
      <VoiceId>3b0000cc</VoiceId>
      <JapaneseText>ひとりで世界中の不幸を
背負ったような顔してんじゃねえ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>588</PointerOffset>
      <VoiceId>3b0000cd</VoiceId>
      <JapaneseText>おまえに……
オレの何がわかる!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>645</PointerOffset>
      <VoiceId>3b0000ce</VoiceId>
      <JapaneseText>わかんねえ……わかんねえよ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>667</PointerOffset>
      <VoiceId>3b0000cf</VoiceId>
      <JapaneseText>おまえみたいに黙ってたら、
何にもわかりっこねぇだろうが!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>697</PointerOffset>
      <VoiceId>3b0000d0</VoiceId>
      <JapaneseText>何を言えというんだ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>728</PointerOffset>
      <VoiceId>3b0000d1</VoiceId>
      <JapaneseText>かっこつけてんじゃねえ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>759</PointerOffset>
      <VoiceId>3b0000d2</VoiceId>
      <JapaneseText>苦しいんなら苦しいって言えよ!!
つらいんならつらいって言えよ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>790</PointerOffset>
      <VoiceId>3b0000d3</VoiceId>
      <JapaneseText>おまえのその態度が、クレアを
追い詰めたんじゃねえのかよ！
そうだろ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>835</PointerOffset>
      <VoiceId>3b0000d4</VoiceId>
      <JapaneseText>……オレは……オレは……
うおぉぉぉぉぉぉぉッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>994</PointerOffset>
      <VoiceId>3b0000d8</VoiceId>
      <JapaneseText>うおりゃああああッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1015</PointerOffset>
      <VoiceId>3b0000d9</VoiceId>
      <JapaneseText>ぐあッ……!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1037</PointerOffset>
      <VoiceId>3b0000da</VoiceId>
      <JapaneseText>それでおしまいかよ……
まだまだ、しゃべり足りなそうだぜ、
おまえの拳はよぉ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1081</PointerOffset>
      <VoiceId>3b0000db</VoiceId>
      <JapaneseText>うおおおおおぉッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1112</PointerOffset>
      <VoiceId>3b0000dc</VoiceId>
      <JapaneseText>まだまだぁぁぁぁッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1133</PointerOffset>
      <VoiceId>3b0000dd</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1155</PointerOffset>
      <VoiceId>3b0000de</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1189</PointerOffset>
      <VoiceId>3b0000d5</VoiceId>
      <JapaneseText>うおぉぉぉぉぉッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1220</PointerOffset>
      <VoiceId>3b0000d6</VoiceId>
      <JapaneseText>おああぁぁぁッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1251</PointerOffset>
      <VoiceId>3b0000d7</VoiceId>
      <JapaneseText>……イ、イテテ……
ずいぶんとおしゃべりじゃねえか……
おまえの拳はよぉ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1457</PointerOffset>
      <VoiceId>3b0000df</VoiceId>
      <JapaneseText>&lt;Veigue&gt;!!　&lt;Tytree&gt;!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1544</PointerOffset>
      <VoiceId>3b0000e0</VoiceId>
      <JapaneseText>&lt;Tytree&gt;さん……
どうして、こんな事……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1591</PointerOffset>
      <VoiceId>3b0000e1</VoiceId>
      <JapaneseText>ムッツリの&lt;Veigue&gt;ちゃんと
語るには、拳っきゃねーだろ？
イテテテ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1647</PointerOffset>
      <VoiceId>3b0000e2</VoiceId>
      <JapaneseText>……誰がムッツリだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1678</PointerOffset>
      <VoiceId>3b0000e3</VoiceId>
      <JapaneseText>……だったら、話せよ……
おまえの気持ちをよ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1700</PointerOffset>
      <VoiceId>3b0000e4</VoiceId>
      <JapaneseText>拳で言えるんなら、
口でも言えるだろ？
この痛みの分な……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1721</PointerOffset>
      <VoiceId>3b0000e5</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1830</PointerOffset>
      <VoiceId>3b0000e6</VoiceId>
      <JapaneseText>……わからないんだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1851</PointerOffset>
      <VoiceId>3b0000e7</VoiceId>
      <JapaneseText>オレは……
オレはどうすればいいんだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1872</PointerOffset>
      <VoiceId>3b0000e8</VoiceId>
      <JapaneseText>……ミルハウストはオレに言った……
ベルサスでオレが救ったのは
誰だったのかと……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1907</PointerOffset>
      <VoiceId>3b0000e9</VoiceId>
      <JapaneseText>あの時、オレには……
アガーテがクレアに見えた……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1928</PointerOffset>
      <VoiceId>3b0000ea</VoiceId>
      <JapaneseText>どんな姿をしていようと、
心がクレアならそれがクレアだと、
あの時、オレはそう思った……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1957</PointerOffset>
      <VoiceId>3b0000eb</VoiceId>
      <JapaneseText>でも、クレアがクレアらしく
ふるまえばふるまうほど……
アガーテの姿である事が気になるんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1992</PointerOffset>
      <VoiceId>3b0000ec</VoiceId>
      <JapaneseText>……まっすぐに
あいつを見られないんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2013</PointerOffset>
      <VoiceId>3b0000ed</VoiceId>
      <JapaneseText>それが……それがあいつを……
クレアを傷つけていると……
わかっていたのに……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2047</PointerOffset>
      <VoiceId>3b0000ee</VoiceId>
      <JapaneseText>……オレは……オレは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2087</PointerOffset>
      <VoiceId>3b0000ef</VoiceId>
      <JapaneseText>……やっと本音が出たな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2108</PointerOffset>
      <VoiceId>3b0000f0</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2186</PointerOffset>
      <VoiceId>3b0000f1</VoiceId>
      <JapaneseText>そんなにがんばるなよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2213</PointerOffset>
      <VoiceId>3b0000f2</VoiceId>
      <JapaneseText>…………？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2244</PointerOffset>
      <VoiceId>3b0000f3</VoiceId>
      <JapaneseText>おまえが苦しんでるのは
わかってた。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2266</PointerOffset>
      <VoiceId>3b0000f4</VoiceId>
      <JapaneseText>でも、人前で弱音を吐くような
おまえじゃねえしな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2297</PointerOffset>
      <VoiceId>3b0000f5</VoiceId>
      <JapaneseText>だけどよぉ……
暴走するまで、ひとりで
がんばる事はねぇだろうよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2318</PointerOffset>
      <VoiceId>3b0000f6</VoiceId>
      <JapaneseText>&lt;Tytree&gt;……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2349</PointerOffset>
      <VoiceId>3b0000f7</VoiceId>
      <JapaneseText>おれたちにも、ちったあ
カッコつけさせてくれよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2371</PointerOffset>
      <VoiceId>3b0000f8</VoiceId>
      <JapaneseText>一緒に悩ませてくれよ。
……仲間だろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2405</PointerOffset>
      <VoiceId>3b0000f9</VoiceId>
      <JapaneseText>……オレは……オレは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2511</PointerOffset>
      <VoiceId>3b0000fa</VoiceId>
      <JapaneseText>……ああぁぁ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4185</PointerOffset>
      <JapaneseText>&lt;speed:00&gt;&lt;scale:0140&gt;休憩しますか？
　はい　
　いいえ　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5853</PointerOffset>
      <JapaneseText>&lt;Green&gt;&lt;scale:0140&gt;&lt;speed:00&gt;&lt;font:80000002&gt;&lt;Unk19:3404&gt;が仲間になりました。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
