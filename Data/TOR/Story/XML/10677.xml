      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2003</PointerOffset>
      <VoiceId>1d0000c0</VoiceId>
      <JapaneseText>ジベールさんに頼まれて来た。
あんたがハックだな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2031</PointerOffset>
      <VoiceId>1d0000c1</VoiceId>
