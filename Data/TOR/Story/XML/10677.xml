      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>874</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;が……&lt;Eugene&gt;を
