      <EnglishText/>
      <Notes/>
      <Id>2519</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2961</PointerOffset>
      <JapaneseText>そりゃ、久しぶりの空だもん！</JapaneseText>
